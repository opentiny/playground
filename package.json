{
  "name": "root-playground",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "sync-genui-prod-env": "cp .genui-production.env genui-sdk/sites/playground/web/env/.env",
    "sync-genui-alpha-env": "cp .genui-alpha.env genui-sdk/sites/playground/web/env/.env",
    "predev": "pnpm run sync-genui-alpha-env",
    "dev": "vitepress dev",
    "build": "pnpm -C genui-sdk run build:playground && vitepress build",
    "preview": "vitepress preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^2.1.1",
    "less": "^4.4.1",
    "medium-zoom": "^1.1.0",
    "vitepress": "^1.6.3",
    "vitepress-demo-plugin": "^1.4.2"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "@opentiny/vue-theme": "3.28.0"
  }
}
